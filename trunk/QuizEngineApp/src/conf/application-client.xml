  <ejb-ref>
    <ejb-ref-name>TestEntryBean</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <remote>com.equinox.ejb.session.TestEntryRemote</remote>
  </ejb-ref>
  <ejb-ref>
    <ejb-ref-name>ResultBeanBean</ejb-ref-name>
